{
  "compilerOptions": {
    "allowJs": true,
    "checkJs": true,
    "strict": true,
    "target": "ES2022",
    "module": "ES2022",
    "moduleResolution": "Bundler",
    "incremental": true,
    "tsBuildInfoFile": ".tsbuildinfo",
    "paths": {
      "#": ["./src/"]
    },
    "skipLibCheck": true,
    "types": ["expect"]
  }
}
